        "@capacitor/status-bar": "^6.0.3",
        "@tanstack/react-query": "^5.17.0",
        "axios": "^1.6.5",
        "clsx": "^2.1.0",
        "dompurify": "^3.3.1",
        "jszip": "^3.10.1",
        "lucide-react": "^0.303.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.21.0",
        "recharts": "^2.10.3"
    },
    "devDependencies": {
        "@types/dompurify": "^3.0.5",
